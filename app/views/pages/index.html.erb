
<h1 id="white-text"> Welcome!</h2>
  <div id="buttons">
		  <div id="text-center">
        <div class="btn" style="inline-block">
     	    <% if logged_in? %>
       	  		<%= "Logged in as #{current_user.email}" %>
       	   		<%= link_to "(logout)", logout_path, :class=>"button_to"%>
        	<% else %>
          		<%= button_to "Login", login_path, :class=>"button_to", :method => "get"%>
        	<% end %>
   		    <%= button_to 'New User', new_user_path, :class => "btn", :method => "get"%>
        </div>
   	  </div>
  </div>
